@model MyQuickDesk.Entities.Reservation

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Reservation</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <label asp-for="StartTime"></label>
        </dt>
        <dd class="col-sm-10">
            @Model.StartTime
        </dd>
        <dt class="col-sm-2">
            <label asp-for="EndTime"></label>
        </dt>
        <dd class="col-sm-10">
            @Model.EndTime
        </dd>

        <input type="hidden" name="spaceId" value="@ViewBag.SpaceId" />
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a>
    <a class="back-to-list-btn" asp-action="Index">Back to List</a>
</div>



